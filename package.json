{
  "name": "@magnit-ce/path-router",
  "version": "0.3.3",
  "description": "A custom html element that routes to pages based on its path attribute",
  "main": "dist/path-router.umd.js",
  "module": "dist/path-router.js",
  "types": "dist/path-router.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test:build": "vite build --config vite.config.tests.ts",
    "test": "npm run test:build && web-dev-server --watch --app-index test-runner/index.html",
    "changeset": "npx @changesets/cli",
    "release": "npm run build && npx @changesets/cli publish"
  },
  "keywords": [
    "custom",
    "element",
    "html",
    "browser",
    "component",
    "navigation",
    "routing",
    "router",
    "path",
    "tab",
    "page",
    "layout"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@rollup/plugin-terser": "^0.4.4",
    "@web/dev-server": "^0.4.6",
    "ce-part-utils": "^0.0.4",
    "changesets": "^1.0.2",
    "glob": "^13.0.0",
    "typescript": "^5.5.3",
    "vite": "^7.3.0",
    "vite-plugin-dts": "^4.5.4"
  },
  "author": "catapart",
  "license": "CC0-1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/catapart/magnitce-path-router"
  }
}
